{
	"manifest_version": 2,
	"name": "HideMyNavigation",
	"version": "0.0.1",
	"description": "Hides navigation and puts it onto page previews",
        "permissions": ["tabs", "contextMenus", "menus", "activeTab"],
	"background": {
           "scripts": ["browser.js"]
        },
        "browser_action": {
          "default_title": "Navigation",
          "default_popup": "nav.html"
        }
}

